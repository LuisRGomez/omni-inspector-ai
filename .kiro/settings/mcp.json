{
  "mcpServers": {
    "ssh": {
      "command": "npx.cmd",
      "args": [
        "ssh-mcp",
        "-y",
        "--",
        "--host=YOUR_HOST",
        "--port=22",
        "--user=YOUR_USER",
        "--password=YOUR_PASSWORD"
      ],
      "disabled": false,
      "autoApprove": []
    }
  }
}
